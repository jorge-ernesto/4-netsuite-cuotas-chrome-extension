{
  "manifest_version": 3,
  "name": "Netsuite Cuotas Chrome Extension",
  "version": "1.0",
  "description": "Esta es la descripción de mi extensión",
  "icons": {
    "128": "assets/img/logo5.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "scripting"
